    "Northwind": "Data Source=~/data/sqlite/northwind.db;Cache=Shared;Pooling=false;",
    "Northwind2": "Data Source=~/data/sqlite/northwind2.db;Cache=Shared;Pooling=false;",
    "Chinook": "Data Source=~/data/sqlite/chinook.db;Cache=Shared;Pooling=false;",
    "Sakila": "Data Source=~/data/sqlite/sakila.db;Cache=Shared;Pooling=false;",
    "DbNetSuiteCore": "Data Source=~/data/sqlite/dbnetsuitecore.db;Cache=Shared;Pooling=false;"
  },
  "DbNetSuiteCore": {
    "AllowConnectionString": "false",
    "UpdateDisabled": "false",
    "StateManagement": "Client",
    "UseDistributedServerCache": "false"
  }
