{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "northwind": "Data Source=~/data/northwind.db;Cache=Shared",
    "chinook": "Data Source=~/data/chinook.db;Cache=Shared",
    "sakila": "Data Source=~/data/sakila.db;Cache=Shared",
    "dbnetsuitecore": "Data Source=~/data/dbnetsuitecore.db;Cache=Shared"
  },
  "DbNetSuiteCore": {
    "AllowConnectionString": "false",
    "UpdateDisabled": "false",
    "StateManagement": "Client",
    "UseDistributedServerCache": "false"
  },
  "AllowedHosts": "*"
}
