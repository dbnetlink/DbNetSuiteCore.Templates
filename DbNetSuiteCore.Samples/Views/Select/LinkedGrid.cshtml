@using DbNetSuiteCore.Enums
@using DbNetSuiteCore.Models
@using DbNetSuiteCore.Samples.Helpers
@model DbNetSuiteCore.Samples.ViewModels.SampleModel
@section Title
{
    <p data-summary>A @RazorHelper.HelpLink("select-overview", "Select") control can also link to a @RazorHelper.HelpLink("grid-overview", "Grid") control using the @RazorHelper.HelpLink("selectmodel-properties#linkedcontrolcomponentmodel", "LinkedControl") property</p>
}
@section Control {
@{
    @(await DbNetSuiteCore.Control.Create(Context).Render(Model.SelectModel))
    @(await DbNetSuiteCore.Control.Create(Context).Render(Model.GridModels[0]))
    @(await DbNetSuiteCore.Control.Create(Context).Render(Model.GridModels[1]))
}}
